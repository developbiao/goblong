{{define "article-meta"}}
  <p class="blog-post-meta text-secondary">
    Release From <a href="{{ .Link }}" class="font-weight-bold">{{ .CreatedAtDate }}</a>
    by <a href="{{ .User.Link }}" class="font-weight-bold">{{ .User.Name }}</a>
    Category <a href="#"> {{ .Article.CategoryID }} </a>
  </p>
{{ end }}